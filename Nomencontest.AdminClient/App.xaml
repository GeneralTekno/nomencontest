<Application x:Class="Nomencontest.Clients.Admin.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:base="clr-namespace:Nomencontest.Base;assembly=Nomencontest.Base"
             >
    <Application.Resources>
            <base:ToUpperCaseConverter x:Key="ToUpperCaseConverter"/>
            <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
            <base:GreaterThanConverter x:Key="GreaterThanConverter"></base:GreaterThanConverter>
            <base:IsEqualConverter x:Key="IsEqualConverter"></base:IsEqualConverter>
        <FontFamily x:Key="GameBoardFont">/Fonts/GameBoardFont.otf#Univers LT Std 85 XBlk</FontFamily>
        <FontFamily x:Key="TOFOMI">/Fonts/TOFOMI.ttf#TOFOMI</FontFamily>
        <FontFamily x:Key="DigitalReadout">/Fonts/DJB Get Digital.ttf#DJB Get Digital</FontFamily>
    </Application.Resources>
</Application>
